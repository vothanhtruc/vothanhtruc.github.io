import java.util.*;

public class DemoVector {
	public static void main(String[] args) {
		Vector vector = new Vector(3, 2);
		vector.addElement("a");
		vector.addElement("c");
		vector.addElement("e");
		System.out.println("Capacity1: " + vector.capacity());
		vector.addElement("b");
		System.out.println("Capacity2: " + vector.capacity());
		vector.addElement("d");
		vector.addElement("f");
	}
}
